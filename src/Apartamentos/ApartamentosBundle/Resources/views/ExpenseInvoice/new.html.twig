{% extends '::masteradmin.html.twig' %}
{% block stylesheets %}
{{ parent() }}
<link rel="stylesheet" type="text/css" href="{{ asset('css/jquery-ui-1.10.4.custom.css') }}" />
{% endblock %}
{% block javascripts %}
{{ parent() }}
<script type="text/javascript" src="{{ asset('js/jquery.validate.js') }}"></script>
<script src="{{ asset('js/jquery-ui.js') }}"></script>
<script type="text/javascript"src="{{ asset('js/datepicker.js') }}"></script>
{% include 'ApartamentosApartamentosBundle:ExpenseInvoice:validatedocument.html.twig' %}
{% endblock %}
{% block contenido %}
  
    <h2 class="titulo">{{'Nuevo Documento'|trans}}</h2>
    <hr> 

    {{ form_start(form) }}
    {% form_theme form 'LoginLoginBundle:Form:form_errors.html.twig' %}
    <div>
        {{ form_label(form.expenseid) }}<br><br>
        {{ form_widget(form.expenseid) }}
        {{ form_errors(form.expenseid) }}
    </div><br>
    <div>
        {{ form_label(form.file) }}<br>
        {{ form_widget(form.file) }}
        {{ form_errors(form.file) }}
    </div><br>
    <div>
        {{ form_label(form.name) }}
        {{ form_widget(form.name) }}
        {{ form_errors(form.name) }}
    </div><br>
    <div>
        {{ form_label(form.description) }}
        {{ form_widget(form.description)}}
        {{ form_errors(form.description) }}
    </div>
         
 
{{ form_end(form) }}

    <div style="display:table-row;">
          <a href="{{ path('expenseinvoicegrid',{ 'id':app.request.get('id')}) }}"><img style="display:table-cell;vertical-align:middle;" src="{{ asset('images/documents32.png') }}"/><span style="display:table-cell;
 vertical-align:middle;" >{{'Documentos'|trans}}</span></a>

       </div> 
   
{% endblock %}

