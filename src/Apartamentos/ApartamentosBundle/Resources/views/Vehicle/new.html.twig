{% extends '::masteradmin.html.twig' %}
{% block javascripts %}
{{ parent() }}
<script type="text/javascript" src="{{ asset('js/jquery.validate.js') }}"></script>
{% include 'ApartamentosApartamentosBundle:Vehicle:validatevehicle.html.twig' %}   
{% include 'ApartamentosApartamentosBundle:Vehicle:Vehicleajaxscript.html.twig' %}   
{% endblock %}
{% block contenido %}
<div class="large-12 columns">
    <div class="large-12 columns">
      <h2 class="titulo">{{'Nuevo Veh√≠culo'|trans}}</h2>
      <hr>
    </div> 
    {{ form_start(form) }}
    {% form_theme form 'LoginLoginBundle:Form:form_errors_large_12.html.twig' %}
    
    <div class="large-12 columns">
        <div class="large-6 columns">
        {{ form_label(form.companyid) }}
        {{ form_widget(form.companyid)}}
        {{ form_errors(form.companyid) }}
        </div>    
    </div><br>
    <div class="large-12 columns">
        <div class="large-6 columns">
        {{ form_label(form.towerid) }}
        {{ form_widget(form.towerid)}}
        {{ form_errors(form.towerid) }}
        </div>
    </div>
     <div class="large-12 columns">
        <div class="large-6 columns">         
        {{ form_label(form.apartmentid) }}
        {{ form_widget(form.apartmentid)}}
        {{ form_errors(form.apartmentid) }}
        </div>
    </div>
     <div class="large-12 columns">
         <div class="large-6 columns">   
        {{ form_label(form.residentid) }}
        {{ form_widget(form.residentid)}}
        {{ form_errors(form.residentid) }}
         </div>     
    </div>
    
   <div class="large-12 columns">
       <div class="large-6 columns"> 
       {{ form_label(form.platenumber) }}
       {{ form_widget(form.platenumber)}}
       {{ form_errors(form.platenumber) }}
       </div>   
    </div>
    <div class="large-12 columns">
        <div class="large-6 columns"> 
       {{ form_label(form.type) }}
        {{ form_widget(form.type)}}
        {{ form_errors(form.type) }}
        </div>    
    </div>
    <div class="large-12 columns">
         <div class="large-6 columns">  
        {{ form_label(form.brandid) }}
        {{ form_widget(form.brandid) }}
        {{ form_errors(form.brandid) }}
         </div>     
           
    </div>
     <div class="large-12 columns">
        <div class="large-6 columns"> 
        {{ form_label(form.colorid) }}
        {{ form_widget(form.colorid) }}
        {{ form_errors(form.colorid) }}
        </div>    
    </div>
    <div class="large-12 columns">
         <div class="large-6 columns">  
        {{ form_label(form.parkingid) }}
        {{ form_widget(form.parkingid) }}
        {{ form_errors(form.parkingid) }}
         </div>     
           
    </div>
    
    <div class="large-12 columns">
    <div class="large-6 columns">     
    <button type="submit">{{ 'Guardar'|trans}}</button>
    <a href="{{ path('vehiclegrid') }}" class="button">{{'Cancelar'|trans}}</a>
    </div>
    </div>    
    <div>{{ form_errors(form) }}</div>
</div>   
    
{{ form_end(form) }}
{% endblock contenido %}