{% extends '::masteradmin.html.twig' %}
{% block javascripts %}
{{ parent() }}
<script type="text/javascript" src="{{ asset('js/jquery.validate.js') }}"></script>
{% include 'ApartamentosApartamentosBundle:Parking:validateparking.html.twig' %} 
{% include 'ApartamentosApartamentosBundle:Parking:parkingajaxscript.html.twig' %}  
{% endblock %}
{% block contenido %}
<div class="large-12 columns">
    <div class="large-12 columns">
      <h2 class="titulo">{{'Editar Estacionamiento'|trans}}</h2>
      <hr>
    </div> 
    {{ form_start(edit_form) }}
    {% form_theme edit_form 'LoginLoginBundle:Form:form_errors_large_12.html.twig' %}
    
   
    <div class="large-12 columns">
        <div class="large-6 columns">
        {{ form_label(edit_form.companyid)}}
        {{ form_widget(edit_form.companyid)}}
        {{ form_errors(edit_form.companyid) }}
        </div>
    </div>
    <div class="large-12 columns">
        <div class="large-6 columns">
        {{ form_label(edit_form.towerid)}}
        {{ form_widget(edit_form.towerid)}}
        {{ form_errors(edit_form.towerid) }}
         </div>
    </div>
     <div class="large-12 columns">
        <div class="large-6 columns">
        {{ form_label(edit_form.apartmentid) }}
        {{ form_widget(edit_form.apartmentid) }}
        {{ form_errors(edit_form.apartmentid) }}
        </div>
    </div>
   <div class="large-12 columns">
        <div class="large-6 columns">
        {{ form_label(edit_form.number)}}
        {{ form_widget(edit_form.number)}}
        {{ form_errors(edit_form.number) }}
        </div>
   </div>

    <div class="large-12 columns">
        <div class="large-6 columns">
        {{ form_label(edit_form.locationid) }}
        {{ form_widget(edit_form.locationid) }}
        {{ form_errors(edit_form.locationid) }}
        </div>
    </div>
    <div class="large-12 columns">
        <div class="large-6 columns">
        {{ form_label(edit_form.type) }}
        {{ form_widget(edit_form.type) }}
        {{ form_errors(edit_form.type) }}
        </div>
    </div>
    <div class="large-12 columns">
    <div class="large-6 columns">     
    <button type="submit">{{ 'Guardar'|trans}}</button>
    <a href="{{ path('parkinggrid') }}" class="button">{{'Cancelar'|trans}}</a>
    </div>
    </div>    
    <div>{{ form_errors(edit_form) }}</div>
</div>   
{{ form_end(edit_form) }}
{% endblock contenido %}