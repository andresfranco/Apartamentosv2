{% extends '::masteradmin.html.twig' %}
{% block javascripts %}
{{ parent() }}
<script type="text/javascript" src="{{ asset('js/jquery.validate.js') }}"></script>
{% include 'ApartamentosApartamentosBundle:Parking:validateparking.html.twig' %} 
{% include 'ApartamentosApartamentosBundle:Parking:parkingajaxscript.html.twig' %}  
{% endblock %}
{% block contenido %}
  
    <h2 class="titulo">{{ 'Nuevo Estacionamiento'|trans }}</h2>
    <hr> 

    {{ form_start(form) }}
    {% form_theme form 'LoginLoginBundle:Form:form_errors.html.twig' %}
    
    <div>
        {{ form_label(form.companyid)}}
        {{ form_widget(form.companyid)}}
        {{ form_errors(form.companyid) }}
    </div><br>
    <div>
        {{ form_label(form.towerid)}}
        {{ form_widget(form.towerid)}}
        {{ form_errors(form.towerid) }}
    </div><br>
     <div>
        {{ form_label(form.apartmentid) }}
        {{ form_widget(form.apartmentid) }}
        {{ form_errors(form.apartmentid) }}
    </div><br>
    <div>
        {{ form_label(form.number)}}
        {{ form_widget(form.number)}}
        {{ form_errors(form.number) }}
    </div>

    <div>
        {{ form_label(form.locationid) }}
        {{ form_widget(form.locationid) }}
        {{ form_errors(form.locationid) }}
    </div><br>
    <div>
        {{ form_label(form.type) }}
        {{ form_widget(form.type) }}
        {{ form_errors(form.type) }}
    </div><br>
   
     
{{ form_end(form) }}

     <div style="display:table-row;">
          <a href="{{ path('parkinggrid') }}"><img style="display:table-cell;vertical-align:middle;" src="{{ asset('images/parking.png') }}"/><span style="display:table-cell;
 vertical-align:middle;" >{{'Estacionamientos'|trans}}</span></a>

       </div> 
    
   
{% endblock %}
