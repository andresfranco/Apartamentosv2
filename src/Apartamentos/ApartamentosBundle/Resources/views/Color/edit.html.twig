{% extends '::masteradmin.html.twig' %}
{% block javascripts %}
{{ parent() }}
<script type="text/javascript" src="{{ asset('js/jquery.validate.js') }}"></script>
{% include 'ApartamentosApartamentosBundle:Color:validatecolor.html.twig' %}
 
{% endblock %}
{% block contenido %}
  
    <h2 class="titulo">{{'Editar Color'|trans}}</h2>
    <hr> 

    {{ form_start(edit_form) }}
    {% form_theme edit_form 'LoginLoginBundle:Form:form_errors.html.twig' %}
    
    <div>
        {{ form_label(edit_form.color) }}
        {{ form_widget(edit_form.color)}}
        {{ form_errors(edit_form.color) }}
    </div>

   
{{ form_end(edit_form) }}

     <div style="display:table-row;">
          <a href="{{ path('colorgrid') }}"><img style="display:table-cell;vertical-align:middle;" src="{{ asset('images/color.png') }}"/><span style="display:table-cell;
 vertical-align:middle;" >{{'Color'|trans}}</span></a>

       </div> 
    
   
{% endblock %}
