{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Reservecalendar</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ entity.title }}</td>
            </tr>
            <tr>
                <th>Start</th>
                <td>{{ entity.start|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>End</th>
                <td>{{ entity.end|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ entity.url }}</td>
            </tr>
            <tr>
                <th>Allday</th>
                <td>{{ entity.allday }}</td>
            </tr>
            <tr>
                <th>Createuser</th>
                <td>{{ entity.createuser }}</td>
            </tr>
            <tr>
                <th>Modifyuser</th>
                <td>{{ entity.modifyuser }}</td>
            </tr>
            <tr>
                <th>Createdate</th>
                <td>{{ entity.createdate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Modifydate</th>
                <td>{{ entity.modifydate|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('reservecalendar') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('reservecalendar_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
