{% extends '::masteradmin.html.twig' %}
{% block javascripts %}
{{ parent() }}
<script type="text/javascript" src="{{ asset('js/jquery.validate.js') }}"></script>
{% include 'LoginLoginBundle:Residentuser:validateresidentuser.html.twig' %}
{% endblock %}
{% block contenido %}
<div class="large-12 columns">
    <div class="large-12 columns">
      <h2 class="titulo">{{'Editar Asociaci√≥n'|trans}}</h2>
      <hr>
    </div> 
    {{ form_start(edit_form) }}
    {% form_theme edit_form 'LoginLoginBundle:Form:form_errors_large_12.html.twig' %}
    
    <div class="large-12 columns">
        <div class="large-6 columns">
        {{ form_label(edit_form.userid) }}
        {{ form_widget(edit_form.userid)}}
        {{ form_errors(edit_form.userid)}}
        </div>    
    </div>
     <div class="large-12 columns">
        <div class="large-6 columns">
        {{ form_label(edit_form.residentid) }}
        {{ form_widget(edit_form.residentid)}}
        {{ form_errors(edit_form.residentid) }}
        </div>    
    </div>
    <div class="large-12 columns">
    <div class="large-6 columns">     
    <button type="submit">{{ 'Actualizar'|trans}}</button>
    <a href="{{path('residentusergrid',{'userid':userid})}}" class="button">{{'Cancelar'|trans}}</a>
    </div>
    </div>    
    {{ form_errors(edit_form) }}
</div>   
{{ form_end(edit_form) }}
{% endblock contenido %}